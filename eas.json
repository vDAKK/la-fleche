{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "22.12.0",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_USE_LOCKFILE": "0",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "npm_config_legacy_peer_deps": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "22.12.0",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_USE_LOCKFILE": "0",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "npm_config_legacy_peer_deps": "true"
      }
    },
    "production": {
      "node": "22.12.0",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "enterpriseProvisioning": "adhoc"
      },
      "env": {
        "EXPO_USE_LOCKFILE": "0",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "npm_config_legacy_peer_deps": "true"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./pc-api-key.json"
      },
      "ios": {
        "appleId": "your-apple-id@email.com",
        "ascAppId": "your-app-store-connect-app-id",
        "appleTeamId": "your-team-id"
      }
    }
  }
}
